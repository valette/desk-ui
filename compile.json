{
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source"
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en",
    "fr"
  ],
  "environment": {
    "qx.icontheme": "Tango"
  },
  "applications": [
    {
      "class": "desk.Application",
      "theme": "desk.theme.Theme",
      "name": "desk-ui",
      "type": "browser",
      "bootPath": "source/boot",
      "default": true,
      "environment": {
        "qx.icontheme": "Tango"
      },
      "include": [
        "qx.ui.*",
        "qxl.dialog.*",
        "desk.*"
      ]
    }
  ],
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json"
}
