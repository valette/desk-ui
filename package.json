{
  "name": "EduAnat2",
  "productName": "EduAnat2",
  "description": "EduAnat2",
  "author": "Remi AGIER <remi@aigen.fr> (http://www.aigen.fr), Sebastien Valette <sebastien.valette@creatis.insa-lyon.fr> (http://www.creatis.insa-lyon.fr/~valette)",
  "license": "CECILL-B",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/valette/desk-ui.git"
  },
  "main": "script.js",
  "dependencies": {
    "concat-stream": "^2.0.0"
  },
  "devDependencies": {
    "@qooxdoo/compiler": "^1.0.0-beta.20190530-1110",
    "ace-builds": "1.4.4",
    "acorn": "^6.1.1",
    "async": "3.0.1",
    "bluebird": "3.5.5",
    "c3": "0.7.1",
    "chalk": "2.4.2",
    "css-loader": "3.0.0",
    "d3": "5.9.2",
    "electron": "5.0.3",
    "electron-builder": "^20.39.0",
    "electron-packager": "^13.1.1",
    "heap": "0.2.6",
    "jssha": "2.3.1",
    "jstat": "1.8.3",
    "kdt": "0.1.0",
    "lodash": "4.17.11",
    "numeric": "1.2.6",
    "operative": "0.4.6",
    "random-js": "2.1.0",
    "socket.io-client": "2.2.0",
    "style-loader": "0.23.1",
    "terser": "4.0.0",
    "three": "0.105.2",
    "ttest": "1.1.0",
    "url-loader": "2.0.0",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.4",
    "webworkify-webpack": "2.1.5",
    "xterm": "3.14.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run qooxdoo && npm run bundle && ./extra.sh && npm run package && npm run zip",
    "buildAddons": "node source/buildAddons.js",
    "bundle": "webpack && npm run terser",
    "clean": "rm -rf source-output && rm -rf build && rm -rf dist && rm -rf EduAnat2-*",
    "compile": "qx compile --target source && qx compile --target build",
    "package": "npm run package-app && npm run package-deb-exe",
    "package-app": "electron-packager --all --overwrite --icon=icon.ico --azar=true build/ EduAnat2",
    "package-deb-exe": "electron-builder build -wl --config builder-effective-config.yaml",
    "qooxdoo": "qx package install && npm run compile",
    "terser": "terser source-output/desk-ui/bundle.js --mangle -o build/desk-ui/bundle.js",
    "updateAll": "rm -rf node_modules && git pull && npm install && npm run build",
    "zip": "for i in ./EduAnat2-*; do zip -rqy \"dist/${i%/}.zip\" \"$i\"; done"
  }
}
