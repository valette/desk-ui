{
  "name": "desk-ui",
  "description": "HTML front-end for visualization (3D objects and medical images)",
  "author": "Sebastien Valette <sebastien.valette@creatis.insa-lyon.fr> (http://www.creatis.insa-lyon.fr/~valette)",
  "license": "CECILL-B",
  "version": "2.2.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/valette/desk-ui.git"
  },
  "main": "build/index.html",
  "dependencies": {
    "fs-extra": "9.0.0",
    "shelljs": "0.8.3"
  },
  "devDependencies": {
    "ace-builds": "1.4.9",
    "async": "3.2.0",
    "bluebird": "3.7.2",
    "c3": "0.7.15",
    "chalk": "4.0.0",
    "chroma-js": "2.1.0",
    "css-loader": "3.5.2",
    "d3": "5.15.1",
    "heap": "0.2.6",
    "jssha": "3.0.0",
    "jstat": "1.9.2",
    "kdt": "0.1.0",
    "lodash": "4.17.15",
    "numeric": "1.2.6",
    "operative": "0.4.6",
    "papaya-viewer": "1.0.1449",
    "@qooxdoo/compiler": "1.0.0-beta.20200413-1323",
    "@qooxdoo/framework": "^6.0.0-beta-20200213-1523",
    "random-js": "2.1.0",
    "socket.io-client": "2.3.0",
    "style-loader": "1.1.3",
    "terser": "4.6.11",
    "three": "0.115.0",
    "ttest": "1.1.0",
    "url-loader": "4.1.0",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webworkify-webpack": "2.1.5",
    "xterm": "4.5.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run qooxdoo && npm run bundle",
    "buildAddons": "node source/buildAddons.js",
    "bundle": "node source/build-worker.js && webpack && npm run terser",
    "clean": "rm -rf compiled",
    "compile": "qx compile --target source && qx compile --target build",
    "qooxdoo": "qx pkg update && qx pkg install && npm run compile",
    "terser": "terser compiled/source/desk-ui/bundle.js --mangle -o compiled/build/desk-ui/bundle.js",
    "updateAll": "npm run updateQooxdoo && rm -rf node_modules && git pull && npm install && npm run build",
    "updateQooxdoo": "cd qooxdoo && git pull && cd .."
  }
}
